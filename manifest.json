{
    "name": "Facebook Dark",
    "description": "Makes Facebook dark to stop killing your eyes",
    "version": "0.1",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://*.facebook.com/*"],
            "css": ["dark.css"],
            "js": ["jquery-2.1.1.min.js","dark.js"]
        }
    ]
}
